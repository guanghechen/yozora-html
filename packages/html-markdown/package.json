{
  "name": "@yozora/html-markdown",
  "version": "2.0.0-alpha.2",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/yozora-html.git",
    "directory": "packages/html-markdown"
  },
  "homepage": "https://github.com/guanghechen/yozora-html/tree/main/packages/html-markdown#readme",
  "keywords": [
    "mdast to html",
    "yozora"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "postbuild": "cp src/style.css lib/style.css",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir ."
  },
  "peerDependencies": {
    "@yozora/ast": "^2.0.0"
  },
  "dependencies": {
    "@yozora/core-html-renderer": "^2.0.0-alpha.2",
    "@yozora/html-admonition": "^2.0.0-alpha.2"
  }
