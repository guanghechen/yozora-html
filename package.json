{
  "name": "@yozora/html",
  "version": "0.0.0",
  "private": true,
  "description": "Render yozora markdown ast into html strings.",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/yozora-html.git"
  },
  "homepage": "https://github.com/yozorajs/yozora-html/#readme",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    ":publish": "yarn test && lerna publish",
    ":upgrade": "yarn upgrade-interactive && run-s clean format build test",
    ":workspace:link": "lerna exec 'yarn link' --concurrency 1",
    ":workspace:unlink": "lerna exec 'yarn unlink' --concurrency 1",
    "build": "lerna run build --concurrency 1",
    "clean": "lerna clean --yes && yarn install",
    "format": "run-s format:lintfix format:prettier",
    "format:lintfix": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "prepublishOnly": "pinst --disable",
    "postinstall": "run-s postinstall:dev",
    "postinstall:dev": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "rebuild": "run-s clean build",
    "start": "lerna run start --parallel",
    "test": "lerna run test --concurrency 4",
    "test:coverage": "lerna run test --concurrency 1 -- --coverage",
    "test:update": "lerna run test --parallel -- -u",
    "update:changelog": "gitmoji-changelog update"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@guanghechen/eslint-config": "^2.1.2",
    "@guanghechen/eslint-config-ts": "^2.1.2",
    "@guanghechen/jest-config": "^2.1.2",
    "@guanghechen/rollup-config-tsx": "^2.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.3",
    "@types/prismjs": "^1.26.0",
    "@types/sanitize-html": "^2.6.2",
    "@yozora/ast": "^2.0.3",
    "@yozora/ast-util": "^2.0.3",
    "clsx": "^1.2.1",
    "cross-env": "7.0.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "^10.1.0",
    "gitmoji-changelog": "2.3.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "29.2.1",
    "jest-environment-jsdom": "29.2.1",
    "lerna": "6.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "postcss": "^8.4.18",
    "prettier": "2.7.1",
    "prismjs": "^1.29.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "sanitize-html": "^2.7.2",
    "stylus": "^0.59.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4",
    "yarn-deduplicate": "6.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,eslintrc}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.3.1"
}
