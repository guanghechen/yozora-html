{
  "name": "@yozora/html",
  "version": "0.0.0",
  "private": true,
  "description": "Render yozora markdown ast into html strings.",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/yozora-html.git"
  },
  "homepage": "https://github.com/yozorajs/yozora-html/#readme",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    ":publish": "yarn test && lerna publish",
    ":upgrade": "yarn upgrade-interactive && run-s clean format build test",
    ":workspace:link": "lerna exec 'yarn link' --concurrency 1",
    ":workspace:unlink": "lerna exec 'yarn unlink' --concurrency 1",
    "build": "lerna run build --concurrency 1",
    "clean": "lerna clean --yes && yarn install",
    "format": "run-s format:lintfix format:prettier",
    "format:lintfix": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "prepublishOnly": "pinst --disable",
    "postinstall": "run-s postinstall:dev",
    "postinstall:dev": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "rebuild": "run-s clean build",
    "test": "lerna run test --concurrency 1",
    "test:coverage": "lerna run --concurrency 1 test -- --coverage",
    "test:update": "lerna run --concurrency 1 test -- -u",
    "update:changelog": "gitmoji-changelog update",
    "version": "yarn && git stage yarn.lock"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@guanghechen/eslint-config": "^5.0.0-alpha.0",
    "@guanghechen/eslint-config-ts": "^5.0.0-alpha.0",
    "@guanghechen/jest-config": "^5.0.0-alpha.0",
    "@guanghechen/rollup-config": "^5.0.0-alpha.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/prismjs": "^1.26.0",
    "@types/sanitize-html": "^2.9.0",
    "@yozora/ast": "^2.1.4",
    "@yozora/ast-util": "^2.1.4",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "gitmoji-changelog": "2.3.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lerna": "6.6.1",
    "lint-staged": "13.2.0",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.7",
    "prismjs": "^1.29.0",
    "rimraf": "^4.4.1",
    "rollup": "3.20.2",
    "sanitize-html": "^2.10.0",
    "stylus": "^0.59.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,eslintrc}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.3.1"
}
